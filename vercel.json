{
	"version": 2,
	"builds": [
		{
			"src": "frontend/package.json",
			"use": "@vercel/react"
		},
		{
			"src": "backend/server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/frontend/build/$1"
		},
		{
			"src": "/backend/(.*)",
			"dest": "/backend/server.js"
		}
	]
}
